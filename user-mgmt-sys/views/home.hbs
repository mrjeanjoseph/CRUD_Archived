<table class="table table-bordered">
    <thead>
        <tr>
            <th scope="col">ID</th>
            <th scope="col">First Name</th>
            <th scope="col">Last Name</th>
            <th scope="col">Email</th>
            <th scope="col">Phone</th>
            <th scope="col">Action</th>
        </tr>
    </thead>
    <tbody>
        {{#each rows}}
        <tr>
            <th scope="row">{{this.Id}}</th>
            <td>{{this.first_name}}</td>
            <td>{{this.last_name}}</td>
            <td>{{this.email}}</td>
            <td>{{this.phone}}</td>
            <td>
                <a href="#" type="button" class="btn btn-light btn-small"><i class="bi bi-check"> View</i></a>
                <a href="#" type="button" class="btn btn-light btn-small"><i class="bi bi-pencil"> Edit</i></a>
                <a href="#" type="button" class="btn btn-light btn-small"><i class="bi bi-person-x"> Delete</i></a>
            </td>
        </tr>
        {{/each}}
    </tbody>
</table>